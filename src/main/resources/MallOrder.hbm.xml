<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wechat.entity.MallOrder" table="mallorder">

		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment"></generator>
		</id>

		<property name="orderNumber" type="java.lang.String" column="ordernumber"></property>
	
		<property name="remarks" type="java.lang.String" column="remarks"></property>
		<property name="freight" type="java.lang.Double" column="freight"></property>
		<property name="memberId" type="java.lang.Integer" column="memberid"></property>
		<property name="addressId" type="java.lang.Integer" column="addressid"></property>
		<property name="createDate" type="java.util.Date" column="createdate"></property>
		<property name="status" type="java.lang.Integer" column="status"></property>
		<property name="totalPrice" type="java.lang.Double" column="totalprice"></property>
		<property name="express" type="java.lang.String" column="express"></property>
		<property name="expressNumber" type="java.lang.String" column="expressnumber"></property>
		<property name="operatorId" type="java.lang.Integer" column="operatorid"></property>
		<property name="couponsMoney" type="java.lang.Double" column="couponsmoney"></property>
		<property name="statusBlank" type="java.lang.Integer" column="statusblank"></property>
		<property name="integral" type="java.lang.Integer" column="integral"></property>
	</class>
</hibernate-mapping>