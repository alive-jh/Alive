<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wechat.entity.Material" table="material">

		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment"></generator>
		</id>

		<property name="title" type="java.lang.String" column="title"></property>
		<property name="url" type="java.lang.String" column="url"></property>
		<property name="accountId" type="java.lang.Integer" column="accountid"></property>
		<property name="content" type="java.lang.String" column="content"></property>
		<property name="summary" type="java.lang.String" column="summary"></property>
		<property name="logo" type="java.lang.String" column="logo"></property>
		<property name="type" type="java.lang.Integer" column="type"></property>
		<property name="parentId" type="java.lang.Integer" column="parentid"></property>
		<property name="createDate" type="java.util.Date" column="createdate"></property>
		<property name="contentType" type="java.lang.Integer" column="contenttype"></property>
		<property name="logoStatus" type="java.lang.Integer" column="logostatus"></property>

		

	</class>
</hibernate-mapping>